{
  "name": "made-of-rocks",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --cache src --write --plugin-search-dir=. ./src/**/*.{svelte,ts}",
    "lighthouse": "unlighthouse --config unlighthouse.config.ts",
    "lint": "eslint . --cache"
  },
  "devDependencies": {
    "@formkit/auto-animate": "^0.7.0",
    "@iconify-json/tabler": "^1.1.83",
    "@playwright/test": "^1.36.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-cloudflare": "^2.3.1",
    "@sveltejs/kit": "^1.22.3",
    "@types/animejs": "^3.1.7",
    "@types/marked": "^5.0.1",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "animejs": "^3.2.1",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.14",
    "eslint": "^8.45.0",
    "eslint-plugin-svelte": "^2.32.2",
    "isomorphic-dompurify": "^1.8.0",
    "jszip": "^3.10.1",
    "lightningcss": "^1.21.5",
    "marked": "^5.1.1",
    "marked-smartypants": "^1.0.2",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.26",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "radash": "^11.0.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "svelte-sonner": "^0.1.2",
    "sveltejs-tippy": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "unlighthouse": "^0.9.0",
    "unplugin-icons": "^0.16.5",
    "vite": "^4.4.4",
    "vite-plugin-lightningcss": "^0.0.5",
    "zod": "^3.21.4"
  },
  "type": "module",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/dompurify": "^3.0.2",
    "svelte-multiselect": "^10.1.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "svelteAllowShorthand": true,
    "semi": true,
    "singleQuote": false,
    "svelteStrictMode": true,
    "svelteIndentScriptAndStyle": true,
    "svelteSortOrder": "scripts-options-markup-styles",
    "trailingComma": "none",
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "pluginSearchDirs": false
  },
  "engineStrict": true
}
